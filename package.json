{
	"name": "vue-managesystem-qiankun",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"install:job": "cd subapp-job && npm install",
		"install:language": "cd subapp-language && npm install",
		"install:main": "cd main && npm install",
		"install-all": "npm-run-all install:*",
		"start:language": "cd subapp-language && npm run serve",
		"start:job": "cd subapp-job && npm run serve",
		"start:main": "cd main && npm run serve",
		"start-all": "npm-run-all --parallel start:*"
	},
	"dependencies": {
		"vue": "^2.6.11"
	},
	"devDependencies": {
		"@vue/cli-service": "~5.0.8",
		"npm-run-all": "^4.1.5",
		"vue-template-compiler": "^2.6.11"
	}
}